<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kevts.washington.edu.quizdroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kevts.washington.edu.quizdroid.TopicSelection"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kevts.washington.edu.quizdroid.TopicOverview"
            android:label="@string/title_activity_topic_overview" >
        </activity>
        <activity
            android:name="kevts.washington.edu.quizdroid.QuestionPage"
            android:label="@string/title_activity_question_page" >
        </activity>
        <activity
            android:name="kevts.washington.edu.quizdroid.AnswerSummary"
            android:label="@string/title_activity_answer_summary" >
        </activity>
    </application>

</manifest>